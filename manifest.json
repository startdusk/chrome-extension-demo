{
    "name": "my first extension",
    "version": "1.0.0",
    "description": "This is my very first cool extension",
    "manifest_version": 3,
    "permissions": [
        "bookmarks"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["content-script.js"]
        }
    ]
}